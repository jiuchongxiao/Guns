<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.common.persistence.dao.AppInfoMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.common.persistence.model.AppInfo">
		<id column="id" property="id" />
		<result column="version" property="version" />
		<result column="enable_flag" property="enableFlag" />
		<result column="item_order" property="itemOrder" />
		<result column="date_created" property="dateCreated" />
		<result column="code" property="code" />
		<result column="last_updated" property="lastUpdated" />
		<result column="name" property="name" />
		<result column="extention" property="extention" />
		<result column="description" property="description" />
		<result column="create_user" property="createUser" />
		<result column="update_user" property="updateUser" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, version, enable_flag AS enableFlag, item_order AS itemOrder, date_created AS dateCreated, code, last_updated AS lastUpdated, name, extention, description, create_user AS createUser, update_user AS updateUser
    </sql>

</mapper>
